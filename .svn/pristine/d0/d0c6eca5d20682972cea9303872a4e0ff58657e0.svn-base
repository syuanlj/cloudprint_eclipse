package eleme.openapi.sdk.api.enumeration.order;

public enum EvaluationStatus {
    /**
     * 等待中（不能评价）
     */
    WAITING("WAITING"), 
    
    /**
     * 未评价（可以评价）
     */
    UNCOMMENT("UNCOMMENT"), 
    
    /**
     * 已评价
     */
    COMMENTED("COMMENTED");
    

    private String orderDesc;
    EvaluationStatus(String orderDesc) {
        this.orderDesc = orderDesc;
    }
}